<!-- TABLE -->
<%_ if (numOfPages>0 && colNames.length>0){-%>
<div class="table-responsive table-responsive-sm table-responsive-md">
  <table class="table table-striped">
    <thead>
      <tr>
        <%_ colNames.forEach(col => {-%>
        <th scope="col"><%=col%></th>
        <%_ });-%>
        <%_ if(data[0][primaryKey]){-%>
        <th scope="col">Actions</th>
        <%_}-%>
      </tr>
    </thead>
    <tbody>
      <%_ data.forEach(row => { -%>
      <tr tabindex="-1">
        <%_ colPg.forEach(col => { -%>
        <td>
          <%_ if(col=='projectid'){ -%>
          <a href="projects/overview/<%=row[col]%>"><%=row[col]%></a>
          <%_ } else{ -%>
          <%=row[col]%>
          <%_ } -%>
        </td>
        <%_ }); -%>
        <td>
          <%_ if(row[primaryKey]){-%>
          <a href="<%=url.slice(0,url.indexOf('?'))%>/edit/<%=row[primaryKey]%>" role="button" class="btn btn-success mr-1">
            <i class="fa fa-pencil-square-o" aria-hidden="true"></i>Edit
          </a>
          <%_ if(admin || tableName!=='Projects') {-%>
          <a href="<%=url.slice(0,url.indexOf('?'))%>/delete/<%=row[primaryKey]%>" role="button" class="btn btn-danger">
            <i class="fa fa-trash" aria-hidden="true"></i>Delete
          </a>
          <%_}%>
          <%_}-%>
        </td>
      </tr>
      <%_ }); -%>
    </tbody>
  </table>
</div>
<!-- alert on change -->
<% include ./notif-alert %>
<!-- Pagination -->
<% include ./pagination %>
<%_ } else if (colNames.length==0) {-%>
<h6>No columns displayed</h6>
<%_ } else {-%>
<h6><%=tableName%> are not found</h6>
<%_ } -%>